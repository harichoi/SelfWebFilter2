<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context=".internet.InternetActivity"
    tools:showIn="@layout/activity_internet">

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">
        <FrameLayout
            android:id="@+id/main_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageButton
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:src="@android:drawable/ic_dialog_dialer"
                    android:id="@+id/drawer_button"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true">
                </ImageButton>
                <EditText
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/urlText"
                    android:layout_alignParentTop="true"
                    android:layout_toEndOf="@+id/drawer_button"
                    android:singleLine="true"
                    android:layout_toLeftOf="@+id/urlMoveButton"
                    android:layout_toStartOf="@+id/urlMoveButton"
                    android:selectAllOnFocus="true" />

                <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@android:drawable/ic_media_play"
                    android:id="@+id/urlMoveButton"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true" />
                <ProgressBar android:id="@+id/progressBar"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="5dp"
                    android:layout_below="@+id/urlText"
                    />
                <kr.selfcontrol.selfwebfilter.view.MyWebView
                        android:layout_below="@+id/progressBar"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:id="@+id/webView"/>
            </RelativeLayout>
        </FrameLayout>
        <FrameLayout
        android:id="@+id/target_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">
    </FrameLayout>
    </FrameLayout>


</RelativeLayout>
